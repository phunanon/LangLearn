(import "src/data.ix")
(import "src/lookup.ix")
(import "src/random.ix")
(import "src/examples.ix")
(import "src/classroom.ix")

(satisfy args
  [[]]
    "lookup examples random classroom"
  (comp 0 ["lookup"])
    (lookup (join " " (skip 1 args)))
  (comp 0 ["examples"])
    (present-examples-of (1 args))
  [["random"]]
    (random)
  [["classroom"]]
    (classroom)
  (str (0 args) " unrecognised."))
